{
  "require": {
    "symfony/yaml": "^5.1",
    "vlucas/phpdotenv": "^5.3"
  },
  "require-dev": {
    "phpmd/phpmd": "@stable",
    "phpunit/phpunit": "^9.0",
    "squizlabs/php_codesniffer": "3.5.*"
  },
  "autoload": {
    "psr-4": {
      "App\\Controller\\": "src/controller/",
      "App\\Classes\\": "src/classes/",
      "App\\Model\\Database\\": "src/model/database/",
      "App\\Model\\": "src/model/",
      "Dotenv\\": "vendor/vlucas/phpdotenv/src/",
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}